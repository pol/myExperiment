<h1>New picture</h1>

<%= error_messages_for :picture %>

<% form_for(:picture, :url => pictures_path) do |f| %>
  <p>
    <b>User</b><br />
    <%= f.text_field :user_id %>
  </p>

  <p>
    <b>Data</b><br />
    <%= f.text_field :data %>
  </p>

  <p>
    <%= submit_tag "Create" %>
  </p>
<% end %>

<%= link_to 'Back', pictures_path %>