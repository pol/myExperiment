# MySQL (default setup).  Versions 4.1 and 5.0 are recommended.
defaults: &defaults
  adapter: mysql
  username: root
  password: 
  host: localhost

development:
  database: m2_development
  <<: *defaults

production:
  database: m2_development
  <<: *defaults

# Warning: The test db gets erased when `rake` is run.
#   => don't set this to your production/dev db
test:
  database: m2_test
  <<: *defaults
